{
    "name": "media24si/utilities",
    "description": "Media24 Laravel utilities",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [{
            "name": "Bostjan Oblak",
            "email": "bostjan.oblak@salomon.si"
        },
        {
            "name": "Adam Stradovnik",
            "email": "adam.stradovnik@salomon.si"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Media24si\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
